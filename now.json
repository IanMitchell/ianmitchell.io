{
  "name": "ianmitchell",
  "version": 2,
  "public": true,
  "alias": [
    "ianmitchell.io",
    "www.ianmitchell.io"
  ],
  "routes": [
    {
      "src": "/what-i-use",
      "status": 301,
      "headers": {
        "Location": "/uses"
      }
    },
    {
      "src": "/tools",
      "status": 301,
      "headers": {
        "Location": "/uses"
      }
    },
    {
      "src": "/bookmarks",
      "status": 301,
      "headers": {
        "Location": "https://www.notion.so/ianmitchel1/Web-Development-Bookmarks-54071bba54a243b9b687ea30de0eb4f3"
      }
    },
    {
      "src": "/pokemon",
      "dest": "/projects/pokemon/index.html"
    },
    {
      "src": "/projects/bhs",
      "dest": "/projects/bhs/index.html"
    },
    {
      "src": "/resume",
      "dest": "/projects/resume/index.html"
    },
    {
      "src": "\/\\d{4}\/\\d{2}\/\\d{2}\/(.+)\/?",
      "status": 301,
      "headers": {
        "Location": "/blog/$1"
      }
    }
  ],
  "functions": {
    "pages/api/posts.js": {
      "includeFiles": "posts/**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
