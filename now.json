{
  "name": "ianmitchell",
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "/what-i-use",
      "status": 301,
      "headers": {
        "Location": "/uses"
      }
    },
    {
      "src": "/tools",
      "status": 301,
      "headers": {
        "Location": "/uses"
      }
    },
    {
      "src": "/pokemon",
      "dest": "/projects/pokemon/index.html"
    },
    {
      "src": "/projects/bhs",
      "dest": "/projects/bhs/index.html"
    },
    {
      "src": "/resume",
      "dest": "/projects/resume/index.html"
    },
    {
      "src": "\/\\d{4}\/\\d{2}\/\\d{2}\/(.+)\/?",
      "status": 301,
      "headers": {
        "Location": "/blog/$1"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
